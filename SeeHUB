local function createAntiVoidPart()
    local part = Instance.new("Part")
    part.Size = Vector3.new(100000, 3, 100000)
    part.Position = Vector3.new(0, -10, 0)
    part.BrickColor = BrickColor.new("Blue")
    part.Anchored = true
    part.Transparency = 1
    part.Parent = game.Workspace
end

local function createRedPart()
    local part = Instance.new("Part")
    part.Size = Vector3.new(5, 5, 5)
    part.Position = Vector3.new(0, 5, 0)
    part.BrickColor = BrickColor.new("Bright red")
    part.Anchored = true

    part.Parent = game.Workspace
end

local function createWhitePart()
    local part = Instance.new("Part")
    part.Size = Vector3.new(5, 5, 5)
    part.Position = Vector3.new(0, 5, 0)
    part.BrickColor = BrickColor.new("White")
    part.Anchored = true
    part.Parent = game.Workspace
end

local function createAntiDeadBlockPart()
    local part = Instance.new("Part")
    part.Size = Vector3.new(6, 5, 6)
    part.Position = Vector3.new(-271, -6, 1)
    part.BrickColor = BrickColor.new("Black")
    part.Anchored = true
    part.Transparency = 1
    part.Parent = game.Workspace
end


local function createYellowPart()
    local part = Instance.new("Part")
    part.Size = Vector3.new(5, 5, 5)
    part.Position = Vector3.new(0, 15, 2)
    part.BrickColor = BrickColor.new("Yellow")
    part.Anchored = true
    part.Parent = game.Workspace
end

if game.PlaceId == 6403373529 then
    local Library = loadstring(game:HttpGet("https://raw.githubusercontent.com/xHeptc/Kavo-UI-Library/main/source.lua"))()
    local Window = Library.CreateLib("SeeHub", "DarkTheme")
    local Main = Window:NewTab("Main")
    local Gloves = Window:NewTab("Gloves")
    local Credits = Window:NewTab("Credits")
    local GloveSection = Gloves:NewSection("Gloves")
    local AntiSection = Main:NewSection("Anti")
    local MainSection = Main:NewSection("Main")
    local CreditsSection = Credits:NewSection("Credits")

    CreditsSection:NewButton("ytzsee :D", "DM ytzsee on discord", function()
        warn("ytzsee?") -- Call the function when the button is clicked
    end)
     
    CreditsSection:NewButton("Just", "DM justpicycho on discord", function()
        warn("justpicycho?") -- Call the function when the button is clicked
    end)
    
    GloveSection:NewButton("BRICK Farm", "Go To Safe Area", function()
        createRedPart() -- Call the function when the button is clicked
    end)

    MainSection:NewButton("Infinite Yield FE", "...", function()
        loadstring(game:HttpGet('https://raw.githubusercontent.com/ionlyusegithubformcmods/1-Line-Scripts/main/Infinite%20Yield%20but%20with%20secure%20dex'))()
    end)

    GloveSection:NewButton("SLEEPING Farm", "Go To Safe Area", function()
        createWhitePart() -- Call the function when the button is clicked
    end)

     GloveSection:NewButton("VOODO Farm", "Go To Safe Area", function()
        createYellowPart() -- Call the function when the button is clicked
    end)

    AntiSection:NewButton("Anti-Void", "Defense", function()
        createAntiVoidPart() -- Call the function when the button is clicked
    end)

    AntiSection:NewButton("Anti Dead Block", "Defense", function()
        createAntiDeadBlockPart() -- Call the function when the button is clicked
    end)

end

game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
    if not gameProcessed and input.KeyCode == Enum.KeyCode.E then
        createRedPart() -- Call the function when the E key is pressed
    end
end)

game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
    if not gameProcessed and input.KeyCode == Enum.KeyCode.Q then
        createWhitePart() -- Call the function when the E key is pressed
    end
end)

game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
    if not gameProcessed and input.KeyCode == Enum.KeyCode.R then
        createYellowPart() -- Call the function when the E key is pressed
    end
end)

game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
    if not gameProcessed and input.KeyCode == Enum.KeyCode.F then
        createAntiVoidPart() -- Call the function when the E key is pressed
    end
end)

game:GetService("UserInputService").InputBegan:Connect(function(input, gameProcessed)
    if not gameProcessed and input.KeyCode == Enum.KeyCode.H then
        createAntiDeadBlockPart() -- Call the function when the E key is pressed
    end
end)
